{
  "name": "medical-agent",
  "version": "0.1.0",
  "description": "A VoltAgent application",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --env-file=.env ./src",
    "local-test": "tsx watch --env-file=.env ./local-test",
    "build": "npx tsc",
    "start": "node dist/index.js",
    "lint": "biome check ./src",
    "lint:fix": "biome check --write ./src",
    "typecheck": "tsc --noEmit",
    "volt": "volt"
  },
  "dependencies": {
    "@ai-sdk/amazon-bedrock": "~2.2.8",
    "@aws-sdk/client-dynamodb": "~3.864.0",
    "@aws-sdk/client-eventbridge": "~3.864.0",
    "@aws-sdk/client-s3": "~3.864.0",
    "@aws-sdk/lib-dynamodb": "~3.864.0",
    "@libsql/client": "~0.3.2",
    "@voltagent/cli": "~0.1.10",
    "@voltagent/core": "~0.1.79",
    "@voltagent/logger": "~0.1.3",
    "@voltagent/vercel-ai": "~0.1.18",
    "dotenv": "~16.4.7",
    "libsql": "~0.3.2",
    "zod": "~3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "~1.9.4",
    "@types/node": "~22.10.5",
    "tsx": "~4.19.2",
    "typescript": "~5.7.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
